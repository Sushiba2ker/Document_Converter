{
  "name": "document-converter",
  "version": "1.0.0",
  "description": "Modern document conversion application with Next.js and FastAPI",
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm install && npm run build",
    "start": "cd frontend && npm start",
    "lint": "cd frontend && npm run lint",
    "backend": "python run.py",
    "install-frontend": "cd frontend && npm install",
    "install-backend": "pip install -r requirements.txt"
  },
  "keywords": [
    "document-converter",
    "nextjs",
    "fastapi",
    "docling",
    "pdf-converter",
    "file-conversion"
  ],
  "author": "Document Converter Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/document-converter.git"
  }
}
